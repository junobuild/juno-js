{
  "name": "@junobuild/ic-client",
  "version": "3.1.2",
  "description": "Agent and actor with generated bindings for Juno on the Internet Computer",
  "author": "David Dal Busco (https://daviddalbusco.com)",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.mjs"
    },
    "./actor": {
      "types": "./actor.d.ts",
      "import": "./actor.js",
      "require": "./actor.mjs"
    },
    "./webauthn": {
      "types": "./webauthn.d.ts",
      "import": "./webauthn.js",
      "require": "./webauthn.mjs"
    },
    "./utils": {
      "types": "./utils.d.ts",
      "import": "./utils.js",
      "require": "./utils.mjs"
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "declarations",
    "**/*.js",
    "**/*.js.map",
    "**/*.mjs",
    "**/*.mjs.map",
    "**/*.d.ts",
    "**/*.d.ts.map"
  ],
  "scripts": {
    "copy-declarations": "rm -rf declarations && cp -R src/declarations . && rm declarations/.npmignore",
    "clean": "git ls-files --others --ignored --exclude-standard | grep -v \"LICENSE\" | xargs rm -rf",
    "ts-declaration": "tsc --emitDeclarationOnly -outDir .",
    "build": "npm run clean && npm run copy-declarations && tsc --noEmit && node esbuild.mjs && npm run ts-declaration",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junobuild/juno-js.git",
    "directory": "packages/ic-client"
  },
  "bugs": {
    "url": "https://github.com/junobuild/juno-js"
  },
  "keywords": [
    "juno",
    "internet-computer",
    "ic",
    "dfinity",
    "canister",
    "agent",
    "actor",
    "candid",
    "idl",
    "bindings",
    "sdk",
    "client"
  ],
  "homepage": "https://juno.build",
  "peerDependencies": {
    "@dfinity/utils": "^3.1.0-beta-2025-09-25",
    "@icp-sdk/core": "^4.0.4"
  }
}
